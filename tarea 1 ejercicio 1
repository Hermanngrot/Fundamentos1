#include <iostream>
using namespace std;
int main()
{
    int A, B,t;
    cout<<"Ingrese el valor de A: ";
    cin>>A;
    cout<<"Ingrese el valor de B: ";
    cin>>B;
    cout<<"Valores originales: ";
    cout<<"A = "<<A<<", B = "<<B<<endl;
    t=A;
    A=B;
    B=t;
    cout<<"Los valores intercambiados son: ";
    cout<<"A = "<<A<<", B = "<<B;

    return 0;
}
