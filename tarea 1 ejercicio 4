#include <iostream>
#include <math.h>
using namespace std;
int main(){
    float x1,x2,y1,y2,d;
    cout<<"Ingrese el valor de x1: ";
    cin>>x1;
    cout<<"Ingrese el valor de x2: ";
    cin>>x2;
    cout<<"Ingrese el valor de y1: ";
    cin>>y1;
    cout<<"Ingrese el valor de y2: ";
    cin>>y2;
    d=sqrt(((x2-x1)*(x2-x1))+((y2-y1)*(y2-y1)));
    cout<<"La distancia entre los dos puntos es: "<<d;

    return 0;
}
