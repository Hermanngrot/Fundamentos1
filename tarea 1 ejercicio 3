#include <iostream>
using namespace std;
int main()
{
    int entero;
    float promedio=0;
    cout<<"Ingrese el primer entero: ";
    cin>>entero;
    promedio=entero;
    cout<<"Ingrese el segundo entero: ";
    cin>>entero;
    promedio=promedio+entero;
    cout<<"Ingrese el tercer entero: ";
    cin>>entero;
    promedio=promedio+entero;
    cout<<"Ingrese el cuarto entero: ";
    cin>>entero;
    promedio=promedio+entero;
    cout<<"Ingrese el quinto entero: ";
    cin>>entero;
    promedio=promedio+entero;
    promedio=promedio/5;
    cout<<promedio;
    
    

    return 0;
}
